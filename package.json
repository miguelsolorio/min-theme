{
    "name": "min-theme",
    "displayName": "Min Theme",
    "description": "A minimal theme that comes in dark and light.",
    "version": "1.1.1",
    "publisher": "miguelsolorio",
    "repository": {
        "type": "git",
        "url": "https://github.com/misolori/min-theme.git"
    },
    "engines": {
        "vscode": "^1.25.0"
    },
    "categories": [
        "Themes"
    ],
    "icon": "icon.png",
    "contributes": {
        "themes": [
            {
                "label": "Min Dark",
                "uiTheme": "vs-dark",
                "path": "./themes/min-dark.json"
            },
            {
                "label": "Min Light",
                "uiTheme": "vs",
                "path": "./themes/min-light.json"
            },
            {
                "label": "Min Dark Bold",
                "uiTheme": "vs-dark",
                "path": "./themes/min-dark-bold.json"
            },
            {
                "label": "Min Light Bold",
                "uiTheme": "vs",
                "path": "./themes/min-light-bold.json"
            }
        ]
    },
    "galleryBanner": {
        "color": "#000000",
        "theme": "dark"
    }
}
